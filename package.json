{
  "name": "back",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "build": "npx tsc",
    "build:watch": "npx tsc --watch",
    "client": "cross-env PORT=3002 npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" "
  },
  "dependencies": {
    "concurrently": "^9.1.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "http-proxy-middleware": "^3.0.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "mui-file-input": "^6.0.0",
    "nodemon": "^3.1.7",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "typescript": "^5.7.2"
  }
}
